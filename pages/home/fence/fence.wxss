/* pages/home/fence/fence.wxss */
/* 输入半径大小 */
.set{
  height: 100rpx;
  width: 100%;
  background: #fff;
  border: 1rpx solid #dcdcdc;
  display: flex;
  justify-content: space-around;
  align-items: center;
  border-radius: 40rpx;
  font-size: 30rpx;
  position: absolute;
  top: 0rpx;
  /* box-shadow: 5rpx 5rpx 25rpx #333; */
}
.set image{
  height: 70rpx;
  width: 70rpx;
}
map{
  margin-top: 105rpx;
}
/* 我的位置 */
.my{
  background: #FF5057;
  color: #fff;
  padding: 20rpx;
  border-radius: 10rpx;
  font-size: 35rpx;
  width: 170rpx; 
  text-align: center;
  position: absolute;
  top: 80rpx;
  right: 50rpx;
}
/* 重置 */
.reset{
  background: #FF5057; 
  color: #fff;
  padding: 20rpx;
  border-radius: 10rpx; 
  font-size: 35rpx;
  width: 130rpx; 
  text-align: center;
  position: absolute;
  top: 180rpx;
  right: 70rpx;
}
/* 提交设置 */
.submit{
  height: 100rpx;
  width: 100%;
  position: absolute;
  bottom: 190rpx;
  /* border: 1rpx solid #dcdcdc; */
}
.submit button{
  background: #FF5057;
  color: #fff;
  font-size: 35rpx;
  width: 250rpx; 
}