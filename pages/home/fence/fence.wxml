<!--pages/home/fence/fence.wxml-->
<view class='set'>
      <image src='../../../image/fence.svg'></image>
      <input placeholder='输入半径'></input>
      <text>米</text>
  </view>
<map id="map" longitude="106.523855" latitude="29.817518" scale="14" controls="{{controls}}" bindcontroltap="controltap" markers="{{markers}}" bindmarkertap="markertap" polyline="{{polyline}}" bindregionchange="regionchange" show-location style="width: 100%; height: 650px;">
  <cover-view class='my'>我的位置</cover-view>
  <cover-view class='reset'>重置</cover-view>
  <cover-view class='submit'>
    <button>提交设置</button>
  </cover-view>
</map> 
